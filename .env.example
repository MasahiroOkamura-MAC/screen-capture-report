# Gemini API Key (Get from Google AI Studio)
GEMINI_API_KEY=your_gemini_api_key_here

# Analysis Model
ANALYSIS_MODEL_NAME=gemini-2.5-flash

# Nanobanana Pro API (for Diagram Generation)
# If NANOBANANA_API_KEY is not set, it will fallback to GEMINI_API_KEY
NANOBANANA_API_URL=https://generativelanguage.googleapis.com/v1beta/models/nano-banana-pro-preview:generateContent
NANOBANANA_MODEL_NAME=nano-banana-pro-preview
# NANOBANANA_API_KEY=your_nanobanana_api_key_here (Optional)

# Email Settings (Gmail Example)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
EMAIL_FROM=your_email@gmail.com
EMAIL_TO=recipient_email@example.com
